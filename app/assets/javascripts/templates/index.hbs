<div class='admin_trips span-12'>
  {{#each trip in controller}}
  <div class='trip'>
    <div class='header'>{{trip.name}}</div>
    <div class='dates'>{{trip.start_date}} - {{trip.end_date}}</div>
    <div class='price'>{{trip.price}}</div>
  </div>
  {{/each}}
</div>
<div class="detail_view span-12">
  <h2>Details</h2>
</div>
